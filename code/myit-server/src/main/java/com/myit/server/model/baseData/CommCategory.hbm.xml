<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- <!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 
	3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> -->
<hibernate-mapping>
	<class name="com.myit.server.model.commodity.Commodity" table="T_COMM_CATEGORY"
		lazy="false">
		<id name="id" type="long" column="ID">
			<generator class="increment" />
		</id>

		<property name="categotyCode" type="string" column="CATEGORY_CODE" not-null="true" length="10" />
		<property name="categotyName" type="string" column="CATEGORY_NAME" not-null="true" length="100" />

		<property name="createTime" type="timestamp" column="CREATE_TIME" not-null="true" />
		<property name="lastModified" type="timestamp" column="LAST_MODIFIED"/>
		<property name="lastModifier" type="string" column="LAST_MODIFIER"/>
		<property name="status" type="string" column="STATUS" not-null="true" />
		
	</class>
</hibernate-mapping>